     "text": [
      "153\n",
      "153 is an armstrong number\n"
     ]
    }
   ],
   "source": [
    "x=input()\n",
    "y=int(x)\n",
    "\n",
    "sum=0\n",
    "\n",
    "for i in x:\n",
    "  sum=sum+int(i)**3\n",
    "\n",
    "if sum==y:\n",
    "  print(sum,'is an armstrong number')"
   ]
  }
